{
    "name": "nguyendachuy/laravel-analytics",
    "description": "A Laravel package to retrieve Google Analytics data. Edited to be able to use for lower php versions",
    "keywords": [
        "google",
        "php 7",
        "analytics",
        "retrieve",
        "reports",
        "laravel",
        "Google Analytics 4"
    ],
    "homepage": "https://github.com/nguyendachuy/laravel-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Dac Huy",
            "email": "huy.cit@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "google/analytics-data": "^0.9.4"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "NguyenHuy\\Analytics\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NguyenHuy\\Analytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "NguyenHuy\\Analytics\\Facades\\Analytics"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NguyenHuy\\Analytics\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
